{
  "/A/B/C": {
    "FileSources": {
      "raw": {
        "PathSegments": [
          "'DATA'",
          "yyyy-MM"
        ],
        "FileTemplate": "yyyy-MM-dd_HH-mm-ss'.csv'",
        "FilePeriod": "00:00:10",
        "UtcOffset": "00:00:00",
        "AdditionalProperties": {
          "SamplePeriod": "00:00:01",
          "Separator": ";",
          "DecimalSeparator": ".",
          "InvalidValue": "999",
          "CodePage": 0,
          "HeaderRow": 2,
          "SkipColumnPattern": "Date|Time|IgnoreMe",
          "UnitPattern": "in (.*)",
          "DefaultGroup": "raw",
          "GroupPattern": null,
          "ReplaceNameRules": [
            {
              "Pattern": "Foo",
              "Replacement": "Bar"
            },
            {
              "Pattern": " in (.*)",
              "Replacement": ""
            }
          ]
        }
      }
    }
  }
}